      <PackageReference Include="BatMap" Version="1.1.3" />
      <PackageReference Include="FastExpressionCompiler" Version="1.10.1" />
      <PackageReference Include="FluentValidation" Version="8.0.100" />
      <PackageReference Include="Mapster" Version="3.2.0" />
      <PackageReference Include="TinyMapper" Version="3.0.2-beta" />
      <PackageReference Include="Valit" Version="2.0.0" />
   </ItemGroup>

</Project>
